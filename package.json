{
  "name": "hipster-tools",
  "version": "0.0.1",
  "description": "Web app created with NestJS and VueJS for many Tabletop RPG pourposes",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel start:frontend start:backend",
    "start:frontend": "cd frontend && npm run dev",
    "start:backend": "cd backend && npm run start:dev",
    "start:prod": "cd backend && npm run start:prod",
    "build": "npm-run-all --parallel build:frontend build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && npm run build",
    "deploy": "npm-run-all --parallel deploy:frontend deploy:backend",
    "deploy:frontend": "cd frontend && npm run deploy",
    "deploy:backend": "cd backend && npm run deploy",
    "push:prod": "push-prod.sh"
  },
  "keywords": [
    "rpg",
    "tabletop",
    "tools",
    "hipster"
  ],
  "author": "dudematthew",
  "license": "UNLICENSED",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  },
  "dependencies": {
    "zlib": "^1.0.5"
  },
  "optionalDependencies": {
    "node-canvas": "*"
  }
}
